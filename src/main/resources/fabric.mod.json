{
  "schemaVersion": 1,
  "id": "ycpk",
  "version": "${version}",
  "name": "Sculk & Jaw",
  "description": "A fabric mod adds two sculk family blocks and a new effect called Sculkophobia.",
  "authors": [
    {
      "name": "Cheesecuter"
    },
    {
      "name": "WuYuZeGang"
    }
  ],
  "contact": {
    "sources": "https://github.com/Cheesecuter/SculkJaw",
    "issues": "https://github.com/Cheesecuter/SculkJaw/issues"
  },
  "license": "AGPL-3.0-only",
  "icon": "assets/ycpk/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "ycpk.sculkjaw.client.SculkjawDataGenerator"
    ],
    "client": [
      "ycpk.sculkjaw.client.SculkjawClient"
    ],
    "main": [
      "ycpk.sculkjaw.Sculkjaw"
    ]
  },
  "mixins": [
    "sculkjaw.mixins.json",
    {
      "config": "sculkjaw.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
